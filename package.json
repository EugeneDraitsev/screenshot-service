{
  "name": "screenshot-service",
  "version": "0.1.0",
  "description": "Screenshot service based on Puppeteer and running on AWS Lambda",
  "scripts": {
    "start": "yarn --force && cross-env LOCAL=1 serverless offline start",
    "package:initialize": "cross-env PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true npm install --production",
    "deploy": "sls deploy",
    "deploy:function": "sls deploy function --function png --verbose",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "dependencies": {
    "puppeteer": "^1.6.2",
    "tar": "^4.4.6"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "2.12.0",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-hooks-plugin": "^1.1.0",
    "serverless-offline": "^3.25.6"
  },
  "license": "MIT"
}
