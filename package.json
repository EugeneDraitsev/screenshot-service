{
  "name": "screenshot-service",
  "version": "0.14.0",
  "description": "Screenshot service based on Puppeteer",
  "scripts": {
    "start": "serverless offline start --dontPrintOutput",
    "deploy": "sls deploy",
    "deploy:function": "sls deploy function --function png --verbose",
    "logs": "sls logs --function png --tail",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "dependencies": {
    "chrome-aws-lambda": "^1.14.0",
    "puppeteer-core": "^1.14.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "2.12.0",
    "serverless": "^1.29.2",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-hooks-plugin": "^1.1.0",
    "serverless-offline": "^3.25.6"
  },
  "license": "MIT"
}
