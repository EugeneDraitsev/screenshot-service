{
  "name": "screenshot-service",
  "version": "0.15.0",
  "description": "Screenshot service based on Puppeteer",
  "scripts": {
    "start": "serverless offline start --dontPrintOutput",
    "deploy": "sls deploy",
    "deploy:function": "sls deploy function --function png --verbose",
    "logs": "sls logs --function png --tail",
    "tsc": "tsc --noEmit --skipLibCheck",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint \"src/**\" --fix"
  },
  "dependencies": {
    "chrome-aws-lambda": "^3.1.1",
    "puppeteer-core": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.10.3",
    "@babel/preset-env": "7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@types/puppeteer-core": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.17.1",
    "puppeteer": "^4.0.1",
    "serverless": "^1.74.1",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-hooks-plugin": "^1.1.0",
    "serverless-offline": "^5.12.1",
    "serverless-webpack": "^5.3.2",
    "typescript": "^3.9.5",
    "webpack": "^5.0.0-beta.14",
    "webpack-node-externals": "^1.7.2"
  },
  "license": "MIT"
}
