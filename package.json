{
  "name": "screenshot-service",
  "version": "0.15.0",
  "description": "Screenshot service based on Puppeteer",
  "scripts": {
    "start": "serverless offline start --dontPrintOutput",
    "deploy": "sls deploy",
    "deploy:function": "sls deploy function --function png --verbose",
    "logs": "sls logs --function png --tail",
    "tsc": "tsc --noEmit --skipLibCheck",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint \"src/**\" --fix"
  },
  "dependencies": {
    "chrome-aws-lambda": "^1.19.0",
    "puppeteer-core": "^1.19.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/puppeteer-core": "^1.9.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "puppeteer": "^1.19.0",
    "serverless": "^1.51.0",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-hooks-plugin": "^1.1.0",
    "serverless-offline": "^5.10.1",
    "serverless-webpack": "^5.3.1",
    "typescript": "^3.5.2",
    "webpack": "^4.34.0",
    "webpack-node-externals": "^1.7.2"
  },
  "license": "MIT"
}
