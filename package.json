{
  "name": "screenshot-service",
  "version": "0.15.0",
  "description": "Screenshot service based on Puppeteer",
  "scripts": {
    "start": "serverless offline start --dontPrintOutput",
    "deploy": "sls deploy",
    "deploy:function": "sls deploy function --function png --verbose",
    "logs": "sls logs --function png --tail",
    "tsc": "tsc --noEmit --skipLibCheck",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint \"src/**\" --fix"
  },
  "dependencies": {
    "chrome-aws-lambda": "^2.0.1",
    "puppeteer-core": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.5",
    "@babel/preset-env": "7.7.6",
    "@babel/preset-typescript": "^7.7.4",
    "@types/puppeteer-core": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "puppeteer": "^2.0.0",
    "serverless": "^1.59.3",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-hooks-plugin": "^1.1.0",
    "serverless-offline": "^5.12.1",
    "serverless-webpack": "^5.3.1",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-node-externals": "^1.7.2"
  },
  "license": "MIT"
}
